%div
  %b Name
  %br/
  = f.text_field :name
  
%div
  %b Nickname
  %br/
  = f.text_field :nickname 


%div
  %b Address Line One
  %br/
  = f.text_field :address1, :size => 16
  
%div
  %b Address Line Two
  %br/
  = f.text_field :address2, :size => 16  
  

%div
  %b City
  %br/
  = f.text_field :city

%div
  %b County
  %br/
  = f.text_field :county_name

%div
  %b State
  %br/
  =collection_select("team","state_id",State.all,"id","name",{ :selected => @team.state_name })


%div
  %b Zip/Postal Code
  %br/
  = f.text_field :zip, :size => 12


%div
  %b Description
  %br/
  = f.text_field :description , :size => 65

%div
  %b League
  %br/
  = text_field_with_auto_complete :team, :league_name, :autocomplete => "off"


%div
  %b Phone
  %br/
  = f.text_field :phone, :size => 12

%div
  %b Contact E-mail address
  %br/
  = f.text_field :email, :size => 32



-if current_user.admin?  
  %div
    %b Ad Zone (Be sure to coordinate with ad server zones)
    %br/
    = f.text_field :ad_zone
