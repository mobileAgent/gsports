.profileContentArea
  .profilePageHeader
    %h2.columnDescriptor
      %span
        Staff Accounts
      = @current_user.full_name
  .leftNavCol
    %h2.yellowHeader
      = _('My Profile')
    .leftNav
      =render :partial => "users/profile_nav"

  .listings
  
    .listingItem
      = link_to 'New Staff Member', new_staff_path(), :class => 'genericButton'
  
    = render :partial => 'staff_listing', :collection => @staffs



    
