<h1>New moniker</h1>

<% form_for(@moniker) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user_generated %><br />
    <%= f.check_box :user_generated %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', monikers_path %>
<div class="clearDiv"/>