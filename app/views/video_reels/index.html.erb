<h1>Listing video_reels</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>User</th>
    <th>Dockey</th>
  </tr>

<% for video_reel in @video_reels %>
  <tr>
    <td><%=h video_reel.title %></td>
    <td><%=h video_reel.description %></td>
    <td><%=h video_reel.user_id %></td>
    <td><%=h video_reel.dockey %></td>
    <td><%= link_to 'Show', video_reel %></td>
    <td><%= link_to 'Edit', edit_video_reel_path(video_reel) %></td>
    <td><%= link_to 'Destroy', video_reel, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video_reel', new_video_reel_path %>
