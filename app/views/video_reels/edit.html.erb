<h1>Editing Reel <%=h @video_reel.title%></h1>

<% form_for(@video_reel) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @video_reel %> |
<%= link_to 'Back', video_reels_path %>
