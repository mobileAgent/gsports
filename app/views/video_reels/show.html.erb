<p>
  <b>Title:</b>
  <%=h @video_reel.title %>
</p>

<p>
  <b>Description:</b>
  <%=h @video_reel.description %>
</p>

<p>
  <b>User:</b>
  <%=h @video_reel.user_id %>
</p>

<%= render :partial => 'shared/favorite_item', :locals => { :item => @video_reel } %> |
<%= render :partial => 'shared/favorite_count', :locals => { :item => @video_reel } %> |
<%= render :partial => 'shared/inappropriate_item', :locals => { :item => @video_reel } %> |
<% if current_user && current_user.can_edit?(@video_asset) %>
  <%= link_to 'Edit', edit_video_reel_path(@video_reel) %> |
  <%= link_to 'Destroy', @video_reel, :confirm => 'Are you sure?', :method => :delete %>
<% end %>  
