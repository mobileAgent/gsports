- @section = (params[:metro_area_id] ? 'metro_area' : 'users')

.profileContentArea.listingPage
  .profilePageHeader
    %h2.columnDescriptor
      %span
        BILLING
      = "Details for #{@member.name}"
  .leftNavCol
    = link_to "Billing Home", :controller => :billing
    %br/

    %h2.yellowHeader
      = _('My Profile')
    .leftNav
      =render :partial => "users/profile_nav"
  .listings
    .box
      %table{:border => 0, :cellspacing => 0, :cellpadding => 0, :width => '100%'}
        %thead
          %tr.yellowHeader
            %td.padded.contentBoxHeader.la
              Number(partial) 
            %td.padded.contentBoxHeader.la
              Expiration date (Month/year)
        %tbody
          %tr.forumList
            %td.forumList
              - if @member.credit_card
                =@member.credit_card.displayable_number
            %td.forumList
              - if @member.credit_card
                ="#{@member.credit_card.month}/#{@member.credit_card.year}"
        
      %br.clear
