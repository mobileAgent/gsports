<h1>Listing video_assets</h1>

<table>
  <tr>
    <th>Dockey</th>
    <th>Title</th>
    <th>Description</th>
    <th>Author name</th>
    <th>Author email</th>
    <th>Length</th>
    <th>Frame rate</th>
    <th>Type</th>
    <th>Status</th>
    <th>Can edit</th>
    <th>Thumbnail</th>
    <th>Thumbnail low</th>
    <th>Thumbnail medium</th>
  </tr>

<% for video_asset in @video_assets %>
  <tr>
    <td><%=h video_asset.dockey %></td>
    <td><%=h video_asset.title %></td>
    <td><%=h video_asset.description %></td>
    <td><%=h video_asset.author_name %></td>
    <td><%=h video_asset.author_email %></td>
    <td><%=h video_asset.lenth %></td>
    <td><%=h video_asset.frame_rate %></td>
    <td><%=h video_asset.video_type %></td>
    <td><%=h video_asset.video_status %></td>
    <td><%=h video_asset.can_edit %></td>
    <td><%=h video_asset.thumbnail %></td>
    <td><%=h video_asset.thumbnail_low %></td>
    <td><%=h video_asset.thumbnail_medium %></td>
    <td><%= link_to 'Show', video_asset %></td>
    <td><%= link_to 'Edit', edit_video_asset_path(video_asset) %></td>
    <td><%= link_to 'Destroy', video_asset, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New video_asset', new_video_asset_path %>
